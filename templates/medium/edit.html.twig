{% extends 'base.html.twig' %}

{% block body %}

    <h1>Editer un Medium</h1>

    <div class="jumbotron">
        <div class="container">
            {{ form_start(editMediumForm) }}
            <div class="row">
                <div class="col-md-9"></div>
                <div class="col-md-3">
                    {% include 'scan.html.twig' %}
                </div>
            </div>


            <div class="row">
                <div class="col-md-6">
                    {{ form_row(editMediumForm.nom) }}
                    {{ form_row(editMediumForm.description) }}
                    {{ form_row(editMediumForm.quantity) }}

                </div>
                <div class="col-md-6">

                    {{ form_row(editMediumForm.codebarre) }}
                    {{ form_row(editMediumForm.localisation) }}
                    {{ form_row(editMediumForm.type) }}
                </div>
            </div>
            <div class="row">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <button type="submit" class="btn btn-block btn-primary">Create !</button>
                </div>
            </div>
        </div>
    </div>
    {{ form_end(editMediumForm) }}


{% endblock %}

{% block title %}

{% endblock %}

